{
    "name": "brainbits/blocking-bundle",
    "description": "Object blocking bundle",
    "keywords": [],
    "homepage": "https://www.brainbits.net",
    "license": "MIT",
    "authors": [
        {
            "name": "Stephan Wentz",
            "email": "swentz@brainbits.net"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "brainbits/blocking": "^4.1.0",
        "symfony/config": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "symfony/http-kernel": "^7.0",
        "symfony/routing": "^7.0"
    },
    "require-dev": {
        "brainbits/phpcs-standard": "^7.0",
        "brainbits/phpstan-rules": "^3.0",
        "matthiasnoback/symfony-config-test": "^4.3",
        "matthiasnoback/symfony-dependency-injection-test": "^4.3",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": { "Brainbits\\BlockingBundle\\": "src/" }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
